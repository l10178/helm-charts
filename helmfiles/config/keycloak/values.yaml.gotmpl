auth:
  adminUser: admin
  adminPassword: "admin"
service:
  type: ClusterIP
ingress:
  enabled: true
  hostname: keycloak.{{ .Values.hostnameSuffix }}
  annotations:
    cert-manager.io/cluster-issuer: selfsigned-cert-manager
  tls: true
postgresql:
  enabled: false
externalDatabase:
  host: postgresql.postgresql.svc
  port: 5432
  user: postgres
  password: "postgresql2023"
  database: keycloak
resources:
  requests:
    cpu: 100m
    memory: 128Mi
  limits:
    cpu: 500m
    memory: 512Mi
